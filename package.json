{
    "name": "paradox-syntax",
    "displayName": "Paradox Syntax Highlighting",
    "private": true,
    "description": "Basic syntax highlighting files for paradox games using textmate",
    "author": "Dayshine",
    "contributors": [
        "Gratak",
        "Dayshine"
    ],
    "license": "MIT",
    "version": "0.1.18",
    "preview": true,
    "publisher": "tboby",
    "repository": {
        "type": "git",
        "url": "https://github.com/cwtools/paradox-syntax"
    },
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "hoi4"
            },
            {
                "id": "stellaris"
            },
            {
                "id": "ck2"
            },
            {
                "id": "eu4"
            },
            {
                "id": "imperator"
            },
            {
                "id": "vic2"
            },
            {
                "id": "ck3"
            },
            {
                "id": "vic3"
            },
            {
                "id": "eu5"
            },
            {
                "id": "paradox"
            }
        ],
        "grammars": [
            {
                "language": "stellaris",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "scopeName": "source.stellaris",
                "injectTo": [
                    "source.mod"
                ],
                "path": "./syntaxes/stellaris.tmLanguage.json"
            },
            {
                "language": "hoi4",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "scopeName": "source.hoi4",
                "injectTo": [
                    "source.mod"
                ],
                "path": "./syntaxes/hoi4.tmLanguage.json"
            },
            {
                "language": "eu4",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "scopeName": "source.eu4",
                "injectTo": [
                    "source.mod"
                ],
                "path": "./syntaxes/eu4.tmLanguage.json"
            },
            {
                "language": "ck2",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "scopeName": "source.ck2",
                "injectTo": [
                    "source.mod"
                ],
                "path": "./syntaxes/ck2.tmLanguage.json"
            },
            {
                "language": "imperator",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "language": "vic2",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "language": "ck3",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "language": "vic3",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "language": "eu5",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            },
            {
                "language": "paradox",
                "scopeName": "source.mod",
                "path": "./syntaxes/paradox.tmLanguage.json"
            }
        ],
        "themes": [
            {
                "label": "Stellaris Theme",
                "uiTheme": "vs-dark",
                "path": "./themes/Paradox-Dimmed.tmLanguage.json"
            },
            {
                "label": "Quiet Light Paradox Theme",
                "uiTheme": "vs",
                "path": "./themes/Paradox-QuietLight.tmLanguage.json"
            }
        ]
    }
}
